package draw;

/** ProgramError is an error class extending IllegalArgumentException.
 * @author Ken Szubzda. */

public class ProgramError extends IllegalArgumentException {
    /** message instance variable. */
    private static String message;
    /** Constructer String MSG. */
    public ProgramError(String msg) {
        super(msg);
        message = msg;
    }
    /** getmsg return message. */
    public String getmsg() {
        return message;
    }
}
